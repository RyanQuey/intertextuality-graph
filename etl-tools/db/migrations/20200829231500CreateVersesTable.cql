CREATE TABLE IF NOT EXISTS intertextuality_graph.verses (
  number INT,
  year_written INT, 
  author TEXT,
  osis_ref TEXT, 
  scrollmapper_id TEXT, 
  canonical BOOLEAN, 
  sbl_gnt_text TEXT,
  byz_gnt_text TEXT,
  mt_text TEXT,
  rahlfs_lxx_text TEXT,
  canonical_text TEXT,
  kjv_text TEXT,
  chapter INT, 
  book TEXT, 
  book_series TEXT, 
  testament TEXT, 
  comments TEXT,
  updated_at TIMESTAMP, 
  PRIMARY KEY ((book), chapter, number)
);
