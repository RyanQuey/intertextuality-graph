CREATE TABLE IF NOT EXISTS intertextuality_graph.book_series (
  id UUID, 
  name TEXT, 
  canonical BOOLEAN, 
  author TEXT,
  tyndale_abbreviation TEXT,
  testament TEXT, 
  book_count LIST<TEXT>, 
  book_ids LIST<TEXT>, 
  user_id TEXT, 
  date_written DATE, 
  updated_at TIMESTAMP, 
  comments TEXT,
  PRIMARY KEY ((term), external_api, search_type)) 

  WITH CLUSTERING ORDER BY(external_api ASC, search_type ASC)
);
