CREATE TABLE IF NOT EXISTS intertextuality_graph.chapters (
  number INT,
  date_written DATE, 
  author TEXT,
  id UUID, 
  canonical BOOLEAN, 
  canonical_text TEXT,
  chapter_id TEXT, 
  book_id TEXT, 
  book_series_id TEXT, 
  testament TEXT, 
  user_id TEXT, 
  comments TEXT,
  updated_at TIMESTAMP, 
  PRIMARY KEY ((number), id)) 
);
